class car:
    def __init__(self, brand, fuel_type) -> None:
        self.brand = brand
        self.fuel_type = fuel_type
    
    def drive(self, distance):
        print(f"Driving {self.brand} for {distance}")
    

volvo = car('bmw', 'oil')

##print(f"brand: {volvo.brand}")
##print(f"brand: {volvo.fuel_type}")

volvo.drive(10)